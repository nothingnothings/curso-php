{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.3",
    "psr/container": "^2.0",
    "doctrine/dbal": "^3.3",
    "doctrine/orm": "^2.11",
    "symfony/cache": "^6.0",
    "doctrine/migrations": "^3.4",
    "guzzlehttp/guzzle": "^7.9",
    "twig/twig": "^3.12",
    "twig/intl-extra": "^3.12",
    "slim/slim": "4.*",
    "slim/psr7": "^1.7",
    "psr/http-message": "^2.0",
    "symfony/polyfill-php80": "^1.29",
    "slim/twig-view": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
